{
  "name": "node_project",
  "version": "1.0.0",
  "description": "node.js mentoring program",
  "main": "app.js",
  "scripts": {
    "plain-text-server": "nodemon --exec babel-node src/servers/plain-text-server.js",
    "html-server": "nodemon --exec babel-node src/servers/html-server.js",
    "html-server-stream": "nodemon --exec babel-node src/servers/html-server.js --stream",
    "json-server": "nodemon --exec babel-node src/servers/json-server.js",
    "echo-server": "nodemon --exec babel-node src/servers/echo-server.js",
    "express-server": "nodemon --exec babel-node src/express-app/index.js",
    "express-server-sequelize-seed": "npm run create-db && npm run run-migrations && npm run seed-data && npm run express-server",
    "create-db": "npx sequelize-cli db:create",
    "run-migrations": "npx sequelize-cli db:migrate",
    "seed-data": "npx sequelize-cli db:seed:all",
    "test": ""
  },
